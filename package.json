    "cookie-signature": "^1.0.6",
    "fastify": "^0.37.0",
    "fastify-cookie": "^1.2.0",
    "fastify-mongodb": "^0.3.0",
    "fastify-react": "^1.0.0",
    "fastify-ws": "^0.4.0",
    "file-type": "^7.4.0",
