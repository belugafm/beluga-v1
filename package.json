    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "sh ./build.sh",
        "build_server": "babel server -d www",
        "build_client": "next build client;cp -r client/sounds www;cp -r client/asset www",
        "build_sass": "sh ./build_sass.sh",
        "watch_sass_desktop": "node-sass client/styles/theme/default/desktop/style.scss www/css/theme/default/desktop/style.css --watch  --output-style compressed",
        "watch_sass_mobile": "node-sass client/styles/theme/default/mobile/style.scss www/css/theme/default/mobile/style.css --watch  --output-style compressed",
        "start": "env NODE_ENV=production node www/app.js",
        "initial_settings": "babel-node server/install.js",
        "benchmark": "babel-node server/benchmark.js",
        "test": "env NODE_ENV=test jest"
    },
