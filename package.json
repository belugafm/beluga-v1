    "jsftp": "^2.1.2",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mongodb": "^3.0.1",
    "next": "^4.2.1",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "uid-safe": "^2.1.5",
    "winston": "^3.0.0-rc1"
