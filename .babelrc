{
    "env": {
        "development": {
            "presets": [
                "@babel/preset-env",
                "@babel/preset-react",
                "next/babel"
            ],
            "plugins": [
                [
                    "@babel/plugin-proposal-decorators",
                    {
                        "legacy": true
                    }
                ],
                [
                    "@babel/plugin-proposal-class-properties",
                    {
                        "loose": true
                    }
                ]
            ]
        },
        "production": {
            "presets": [
                "@babel/preset-env",
                "@babel/preset-react",
                "next/babel"
            ],
            "plugins": [
                [
                    "@babel/plugin-proposal-decorators",
                    {
                        "legacy": true
                    }
                ],
                [
                    "@babel/plugin-proposal-class-properties",
                    {
                        "loose": true
                    }
                ]
            ]
        }
    }
}